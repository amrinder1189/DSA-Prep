arr = [1,2,3,4,5,6,7]

# i need to rotate it right 
turn = 3

for i in range(turn):
    temp = arr[0]
    for j in range(len(arr)):
        if j < len(arr)-1:
            arr[j] = arr[j+1]
            
    arr[len(arr)-1] = temp



